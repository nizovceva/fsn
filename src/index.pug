extends pug/base

append vars
	- html.classList.push('page-index')

block content
	section.pt-0
		.container
			.row.align-items-center
				.col-lg-7.text-center
					.page-index-head
						h1 О&nbsp;финансах с&nbsp;гарантией.
						p Финансовая социальная сеть для поиска и&nbsp;инвестирования денежных средств под&nbsp;залог имущества.
						.page-index-buttons
							a.btn.btn-primary.btn-sm.text-uppercase.mr-15(href="#") Инвестировать
							a.btn.btn-outline-success.btn-sm.ml-15(href="#") Разместить займ
						.to-down.d-none.d-sm-flex
							svg(width="40" height="53" viewBox="0 0 40 53" fill="none")
								path(fill-rule="evenodd" clip-rule="evenodd" d="M1.10172 34.9792C0.166908 34.9792 -0.256771 36.1476 0.460785 36.7468L19.1658 52.3653C19.5369 52.6751 20.0765 52.6751 20.4476 52.3653L39.1526 36.7468C39.8702 36.1476 39.4465 34.9792 38.5117 34.9792H36.6099C36.3757 34.9792 36.1488 35.0614 35.969 35.2116L20.4476 48.1718C20.0765 48.4816 19.5369 48.4816 19.1658 48.1718L3.64441 35.2116C3.46458 35.0614 3.23774 34.9792 3.00347 34.9792H1.10172Z")
